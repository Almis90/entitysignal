{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "lib": [ "es2015", "dom", "esnext" ],
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "lib",
    "wwwroot/lib"
  ]
}
